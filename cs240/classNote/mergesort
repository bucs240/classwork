mergesort(array a){
    if a->size ==1 return a;
    left = a[0->n/2];
    right = a[n/2+1->n];
    new_left = mergesort(left);
    new_right = mergesort(right);
    sorted = merge(new_left, new_right)
    return sorted;
}

